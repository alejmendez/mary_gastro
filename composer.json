{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "artesaos/seotools": "^0.9.2",
        "barryvdh/laravel-debugbar": "~2.4",
        "barryvdh/laravel-snappy": "^0.3.2",
        "doctrine/dbal": "v2.5.*",
        "gloudemans/shoppingcart": "^2.3",
        "intervention/image": "dev-master",
        "laravel/framework": "5.4.*",
        "laravelcollective/html": "5.4.*",
        "maatwebsite/excel": "~2.1.0",
        "nwidart/laravel-modules": "^1.19",
        "owen-it/laravel-auditing": "^4.0",
        "roumen/feed": "^2.10",
        "roumen/sitemap": "^2.6",
        "yajra/laravel-datatables-oracle": "7.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "krlove/eloquent-model-generator": "^1.2",
        "laravel/tinker": "~1.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "marygastro\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
